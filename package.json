{
    "name": "resource-pack-converter",
    "version": "1.6.0",
    "description": "A npm package that provides method to convert Minecraft: Java Edition resource packs from a version to another version.",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "tsc",
        "commit": "gitmoji -c",
        "lint": "tslint --project tsconfig.json --config tslint.json src/**/*.ts",
        "lint-fix": "tslint --project tsconfig.json --config tslint.json --fix src/**/*.ts",
        "release": "semantic-release",
        "test": "mocha --require espower-typescript/guess --reporter dot \"./src/test/**/*.ts\"",
        "uti:analyzer": "node uti/analyzer",
        "uti:sorter": "node uti/sorter",
        "uti:whole-getter": "node uti/whole-getter",
        "watch": "tsc -w"
    },
    "bin": {
        "rpc": "lib/index.js",
        "resource-pack-converter": "lib/index.js"
    },
    "directories": {
        "test": "src/test/"
    },
    "dependencies": {
        "canvas": "^2.5.0",
        "fs-extra": "^8.0.1",
        "minimist": "^1.2.0"
    },
    "devDependencies": {
        "@semantic-release/git": "^7.0.12",
        "@types/fs-extra": "^8.0.0",
        "@types/minimist": "^1.2.0",
        "@types/mocha": "^5.2.7",
        "@types/power-assert": "^1.5.0",
        "espower-typescript": "^9.0.0",
        "gitmoji-cli": "^1.9.2",
        "mocha": "^6.0.0",
        "power-assert": "^1.5.0",
        "semantic-release": "^15.13.16",
        "semantic-release-gitmoji": "^1.3.0",
        "ts-node": "^8.0.0",
        "tslint": "^5.17.0",
        "typescript": "^3.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/SPGoding/resource-pack-converter.git"
    },
    "homepage": "https://github.com/SPGoding/resource-pack-converter",
    "keywords": [
        "minecraft"
    ],
    "engines": {
        "node": ">=8.0.0"
    },
    "author": "SPGoding",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SPGoding/resource-pack-converter/issues"
    }
}
